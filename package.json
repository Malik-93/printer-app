{
  "name": "printer-app",
  "bin": "dist/main.js",
  "version": "1.2.0",
  "description": "An application to print rollo receipts.",
  "main": "dist/main.js",
  "engines": {
    "node": ">=16",
    "npm": ">=8",
    "yarn": ">=1"
  },
  "scripts": {
    "prestart": "yarn build",
    "start": "electron dist/main.js",
    "startVirtualPrinters": "ts-node printerTest.ts",
    "dev": "nodemon .",
    "build": "tsc --skipLibCheck && copyfiles index.html dist",
    "makeCert": "cd ./ && for /f \"tokens=3 delims=: \" %i  in ('netsh interface ip show config name^=\"Ethernet\" ^| findstr \"IP Address\"') do mkcert localhost 127.0.0.1 ::1 0.0.0.0 %i",
    "makeExe": "rm -rf ./dist && rm -rf ./exe && yarn build && pkg ."
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js",
      "dist/**/*.json"
    ],
    "assets": [
      "*.zip",
      "*.json"
    ],
    "targets": [
      "node16-macos-x64",
      "node16-win-x64"
    ],
    "outputPath": "exe"
  },
  "author": "Mudassir Malik <malikjrw147@gmail.com>, Guilherme canalli <guilherme.canalli5@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/send": "^0.17.1",
    "axios": "^1.7.4",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "decompress": "^4.2.1",
    "dhcp": "^0.2.20",
    "dotenv": "^16.0.1",
    "express": "^4.19.2",
    "ip": "^1.1.8",
    "ipp": "^2.0.1",
    "ipp-printer": "^1.0.0",
    "local-devices": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "ngrok": "^5.0.0-beta.2",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.2",
    "pdf-to-printer": "^5.4.0",
    "pkg": "^5.8.0",
    "supertest": "^6.2.4",
    "swagger-ui-express": "^4.6.0",
    "unix-print": "^1.2.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/decompress": "^4.2.4",
    "@types/ipp": "^2.0.5",
    "@types/ipp-printer": "^1.0.0",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.8",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "electron": "33.0.2",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
